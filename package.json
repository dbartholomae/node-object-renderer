{
  "name": "object-renderer",
  "version": "1.0.0",
  "description": "A node module to render all properties of an object individually.",
  "keywords": ["requirejs", "client", "validate", "validation"],
  "homepage": "https://github.com/dbartholomae/node-object-renderer",
  "bugs": "https://github.com/dbartholomae/node-object-renderer/issues",
  "license": "MIT",
  "author": "Daniel Bartholomae <daniel@bartholomae.name>",
  "main": "lib/ValidatorRules.js",
  "man": "./doc/index.html",
  "dependencies": {
    "consolidate": "0.12.*",
    "when": "3.*"
  },
  "devDependencies": {
    "mocha": "2.*",
    "chai": "2.*",
    "coffee-script": "1.*",
    "requirejs": "2.*",
    "chai-as-promised": "5.*",
    "dustjs-linkedin": "2.*",
    "jade": "1.*"
  },
  "directories": {
    "lib": "./lib",
    "man": "./doc",
    "example": "./example"
  },
  "repository": {
